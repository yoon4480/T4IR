<!--                        <form>-->

<!--                        </form>-->
                    </li>
                    <li><a href="#">Sell</a></li>
                    <li><a href="#">My Page</a></li>
                    <%
                        if(loginState) {
                    %>
                    <li><a href="#" role="button"><%= loggedInId %></a></li>
                    <li><a href="#" role="button" id="logoutBtn">Logout</a></li>
                    <%
                        } else {
